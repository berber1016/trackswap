{
  "name": "trackswap",
  "version": "0.0.1",
  "description": "A powerful and flexible GPX parser and encoder library for Node.js and TypeScript. TrackSwap provides comprehensive support for GPX 1.1 format with advanced features like plugin system, middleware support, and optimized performance.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test:gpx": "cd example/gpx && rm -f output* test_*.json *_report.json *_encoded.xml && node test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "gpx",
    "gps",
    "parser",
    "encoder",
    "geolocation",
    "track",
    "waypoint",
    "route",
    "typescript",
    "node",
    "xml",
    "geospatial",
    "trackswap"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/buerber1016/trackswap.git"
  },
  "homepage": "https://github.com/buerber1016/trackswap#readme",
  "bugs": {
    "url": "https://github.com/buerber1016/trackswap/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.24",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "htmlparser2": "^10.0.0"
  }
}
